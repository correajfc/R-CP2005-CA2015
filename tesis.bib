	urldate = {2017-03-13},
	journal = {Econometrics},
	author = {LeSage, James P. and Pace, R. Kelley},
	month = dec,
	year = {2014},
	keywords = {indirect effects, sensitivity to spatial weights, spatial regression estimates},
