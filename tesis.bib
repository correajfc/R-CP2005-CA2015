@article{haase_quantitative_2014,
	title = {A {Quantitative} {Review} of {Urban} {Ecosystem} {Service} {Assessments}: {Concepts}, {Models}, and {Implementation}},
	volume = {43},
	issn = {0044-7447, 1654-7209},
